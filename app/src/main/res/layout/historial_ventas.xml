<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/Box"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingLeft="3dp"
    android:orientation="vertical"
    android:textColor="#000000"
    android:shape="rectangle">


    <LinearLayout
        android:id="@+id/Box2"
        android:layout_width="600dp"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:orientation="horizontal"
        android:textColor="#000000"
        android:shape="rectangle">

        <LinearLayout
            android:id="@+id/LLayout_Etiquetas"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/label_DocNum"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|left"
                android:gravity="left"
                android:text="Doc#"
                android:textColor="#000000"
                android:textSize="22sp"
                android:textStyle="normal|bold"></TextView>


            <TextView
                android:id="@+id/label_DocDate"
                android:layout_width="122dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|left"
                android:gravity="center"
                android:text="Fecha"
                android:textColor="#000000"
                android:textSize="22sp"
                android:textStyle="normal|bold"></TextView>


            <TextView
                android:id="@+id/label_Quantity"
                android:layout_width="90dp"
                android:layout_height="wrap_content"

                android:text="Cant"
                android:textColor="#000000"
                android:textSize="22sp"
                android:textStyle="normal|bold"></TextView>


            <TextView
                android:id="@+id/label_PriceBefDi"
                android:layout_width="90dp"
                android:layout_height="wrap_content"

                android:text="Precio"
                android:textColor="#000000"
                android:textSize="22sp"
                android:textStyle="normal|bold"></TextView>




            <TextView
                android:id="@+id/label_DiscPrcnt"
                android:layout_width="70dp"
                android:layout_height="wrap_content"

                android:text="%Desc"
                android:textColor="#000000"
                android:textSize="22sp"
                android:textStyle="normal|bold"></TextView>




            <TextView
                android:id="@+id/label_U_DescFijo"
                android:layout_width="70dp"
                android:layout_height="wrap_content"

                android:text="%Fijo"
                android:textColor="#000000"
                android:textSize="22sp"
                android:textStyle="normal|bold"></TextView>



            <TextView
                android:id="@+id/label_U_DescProm"
                android:layout_width="70dp"
                android:layout_height="wrap_content"

                android:text="%Prom"
                android:textColor="#000000"
                android:textSize="22sp"
                android:textStyle="normal|bold"></TextView>



        </LinearLayout>




    </LinearLayout>

    <LinearLayout
        android:id="@+id/LLayout_Datos"
        android:layout_width="600dp"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:orientation="horizontal"
        android:textColor="#000000"
        android:gravity="left"
        android:shape="rectangle">


        <LinearLayout
            android:id="@+id/Box4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/label_Dato_DocNum"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|left"

                android:gravity="left"
                android:text="1095474"
                android:textColor="#000000"
                android:textSize="22sp"></TextView>

            <TextView
                android:id="@+id/label_Dato_DocDate"
                android:layout_width="122dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|center"

                android:gravity="center"
                android:text="31/12/2019"
                android:textColor="#000000"

                android:textSize="22sp"></TextView>

            <TextView
                android:id="@+id/label_Dato_Quantity"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                    android:textAlignment="gravity"
                android:text="100000"
                android:textColor="#000000"
android:layout_marginLeft="2dp"
                android:textSize="22sp"></TextView>

            <TextView
                android:id="@+id/label_Dato_PriceBefDi"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|left"
android:paddingLeft="2dp"
                android:text="1000000"
                android:textColor="#000000"

                android:textSize="22sp"></TextView>
            <TextView
                android:id="@+id/label_Dato_DiscPrcnt"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|left"

                android:text="100"
                android:textColor="#000000"

                android:textSize="22sp"></TextView>
            <TextView
                android:id="@+id/label_Dato_U_DescFijo"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|left"

                android:text="100"
                android:textColor="#000000"

                android:textSize="22sp"></TextView>
            <TextView
                android:id="@+id/label_Dato_U_DescProm"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|left"

                android:text="100"
                android:textColor="#000000"

                android:textSize="22sp"></TextView>
        </LinearLayout>


    </LinearLayout>





</LinearLayout>